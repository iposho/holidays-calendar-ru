{
  "version": 2,
  "buildCommand": "npm run generate-data",
  "functions": {
    "index.js": {
      "includeFiles": "data/generated/**"
    }
  },
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
